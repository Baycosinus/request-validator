[
    {
        "name": "login",
        "endpoint": "/login",
        "method": "POST",
        "body": [
            {
                "name": "username",
                "type": "string",
                "required": true,
                "minLength": 3,
                "maxLength": 20,
                "regex": "^[a-zA-Z0-9]+$"
            },
            {
                "name": "password",
                "type": "string",
                "required": true,
                "minLength": 8,
                "maxLength": 20,
                "regex": "^[a-zA-Z0-9]+$"
            }
        ]

    }
]